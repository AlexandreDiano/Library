<%= form_with model: @loan do |form| %>
  <%= form.text_field :cpf, value: current_user.profile.id %>
  </br>
  </br>
  <%= fields_for @book do |form| %>
    <%= form.text_field :borrow_books, value: @id%>
  <% end %>
  <%= form.hidden_field :returned_books, value: 0 %>
  <%= form.hidden_field :total_pending, value: 0 %>
  <%= form.submit%>
  <%#= link_to 'Não, eu não quero emprestar este livro!', book_path %>
<% end %>
